<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.SysForePageColMapper">

    <resultMap type="SysForePage" id="SysForePageResult">
        <id     property="id"       column="id"      />
        <result property="page"       column="page"      />
        <result property="status"    column="status"   />

    </resultMap>



    <select id="selectForePageStatus" parameterType="SysForePage" resultMap="SysForePageResult">
		select id,page,status from sys_forepage_col
	</select>










</mapper>