        from villager_info
        <where>
            <if test="aid != null  and aid != '' ">aid in
                <foreach collection="listaid" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach></if>

        </where>
    </select>

    <select id="countbydemob" parameterType="pubObjApi" resultMap="mcountResult">
