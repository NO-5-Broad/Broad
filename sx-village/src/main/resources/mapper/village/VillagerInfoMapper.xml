    <delete id="deleteVillagerInfoByIds" parameterType="String">
        delete from villager_info where vid in 
        <foreach item="vid" collection="array" open="(" separator="," close=")">
            #{vid}
        </foreach>
    </delete>
    <select id="selectPersonAllByApi" parameterType="PersonApi" resultMap="VillagerInfoResult">
        <include refid="selectVillagerInfoVo"/>
        <where>
            <if test="aid != null  and aid != '' ">aid in
                <foreach collection="listaid" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach></if>
            <if test="uname != null  and uname != ''  ">and vname LIKE CONCAT(CONCAT('%', #{uname}),'%')</if>
            <if test="sex != null  and sex != '' "> and sex = #{sex}</if>
        </where>
        ORDER BY vname asc
        limit #{pageSize} offset #{pageIndex}
    </select>

    <select id="selectGrouplist" parameterType="PersonApi" resultMap="VillagerInfoResult">
        select DISTINCT grouptype
        from villager_info
        <where>
            <if test="aid != null  and aid != '' ">aid in
                <foreach collection="listaid" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach></if>
            <if test="politics != null  and politics != '' "> and politics LIKE CONCAT(CONCAT('%', #{politics}),'%')</if>
        </where>
        ORDER BY grouptype asc
        limit #{pageSize} offset #{pageIndex}
    </select>
    <select id="selectvillagerInfoListBytype" parameterType="PersonApi" resultMap="VillagerInfoResult">
        <include refid="selectVillagerInfoVo"/>
        <where>
            <if test="state != null  and state != ''  ">grouptype = #{state}</if>
            <if test="uname != null  and uname != '' "> and vname LIKE CONCAT(CONCAT('%', #{uname}),'%')</if>
            <if test="politics = '%','党员'"> and politics LIKE CONCAT(CONCAT('%',  #{politics}),'%')</if>
        </where>
        ORDER BY vname asc
        limit #{pageSize} offset #{pageIndex}
    </select>

    <select id="countpmBySex" parameterType="PersonApi" resultMap="mcountResult">
        select
        count(vid) as msum,
        count(sex = '男' or null ) as man,
        count(sex = '女' or null ) as woman
        from villager_info
        <where>
            <if test="aid != null  and aid != '' ">aid in
                <foreach collection="listaid" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach></if>
        </where>
    </select>
    <select id="countNewPre" parameterType="pubObjApi" resultMap="mcountResult">
        select
        count(vid) as msum,
        count(age BETWEEN 0 and 2) as sum2
        from villager_info
        <where>
            <if test="aid != null  and aid != '' ">aid in
                <foreach collection="listaid" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach></if>

        </where>
    </select>

    <select id="selectbycadre" parameterType="PersonApi" resultMap="VillagerInfoResult">
        <include refid="selectVillagerInfoVo"/>
        <where>
            <if test="aid != null  and aid != '' ">aid in
                <foreach collection="listaid" item="item" separator="," open="(" close=")">
                    #{item}
                </foreach></if>
            <if test="uname != null  and uname != '' "> and vname LIKE CONCAT(CONCAT('%', #{uname}),'%')</if>
            <if test="sex != null  and sex != ''  ">and sex = #{sex}</if>
            and iscadre=1
        </where>
        ORDER BY vname asc
        limit #{pageSize} offset #{pageIndex}
    </select>

    <select id="countbylowincome" parameterType="pubObjApi" resultMap="mcountResult">
